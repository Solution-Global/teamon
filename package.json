{
  "name": "teamon",
  "productName": "teamon",
  "version": "0.0.1",
  "description": "Team ON",
  "main": "teamon.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Solution-Global/teamon.git"
  },
  "bugs": {
    "url": "https://github.com/Solution-Global/teamon/issues"
  },
  "scripts": {
    "test": "echo 'hello world'",
    "start": "./node_modules/.bin/electron .",
    "clean": "npm run clean:win",
    "pack": "npm run pack:win",
    "clean:win": "rm -rf release/win",
    "pack:win": "npm run clean:win && electron-packager . teamon --out=release/win --platform=win32 --arch=x64 --version=0.36.8 --asar=true --asar-unpack=\"*.node\" --icon=assets/win/teamon.ico --prune --ignore=\"(assets|release)\" --ignore=.gitignore --ignore=packager.json --ignore=\"node_modules/.bin/(asar|electron.*)\" --version-string.ProductName=teamon --overwrite",
    "clean:osx": "rm -rf release/osx",
    "pack:osx": "npm run clean:osx && electron-packager . teamon --out=release/osx --platform=darwin --arch=x64 --version=0.36.8 --asar=true --asar-unpack=\"*.node\" --ignore=\"(assets|release)\" --ignore=.gitignore --ignore=packager.json --ignore='node_modules/.bin/(asar|electron.*)' --overwrite",
    "build:osx": "npm run pack:osx && electron-builder release/osx/teamon-osx --platform=osx --out=release/osx --config=packager.json"
  },
  "author": "Solution-Global",
  "license": "GPL",
  "devDependencies": {
    "electron-builder": "2.6.0",
    "electron-packager": "5.1.1",
    "electron-prebuilt": ">0.36.0",
    "grunt": "0.4.5",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-electron-installer": "2.0.0"
  },
  "dependencies": {
    "jquery": "2.1.4",
    "jquery-mousewheel": "3.1.13",
    "jquery-ui": "1.10.5",
    "jsonfile": "2.2.3",
    "malihu-custom-scrollbar-plugin": "3.1.3",
    "bootstrap": "3.3.6",
    "metismenu": "2.4.0",
    "mqtt": "1.6.3",
    "mustache": "2.2.0",
    "lz-string": "1.4.4",
    "node-rest-client": "1.5.1",
    "dropzone": "4.2.0",
    "cropper": "2.2.5",
    "moment": "2.11.2",
    "moment-timezone": "0.5.0",
    "autosize": "3.0.15"
  }
}
